  (.stable.objs/byte/stable.cmi as stable/stable.cmi)
  (.stable.objs/byte/stable.cmt as stable/stable.cmt)
  (.stable.objs/byte/stable.cmti as stable/stable.cmti)
  (.stable.objs/native/stable.cmx as stable/stable.cmx)
