  (bash
   "%{env:ASPP=aspp-env-var-not-set} %{env:ASPPFLAGS=asppflags-env-var-not-set} -DSYS_%{env:SYSTEM=system-env-var-not-set} -DMODEL_%{env:MODEL=model-env-var-not-set} -o asmgen_amd64.o asmgen_amd64.S")))
